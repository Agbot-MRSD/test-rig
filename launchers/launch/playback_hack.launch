<launch>

	<arg name="bagname" />
	<node pkg="rosbag" type="play" name="playback" output="screen" args="-l $(arg bagname)">
	<remap from="/mapping/left/camera_info" to="/mapping/right/camera_info" />
	<remap from="/mapping/right/camera_info" to="/mapping/left/camera_info" />
	<remap from="/mapping/left/image_raw" to="/mapping/right/image_raw" />
	<remap from="/mapping/right/image_raw" to="/mapping/left/image_raw" />
	</node>
    
</launch>
