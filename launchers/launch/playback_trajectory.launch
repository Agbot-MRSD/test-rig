<launch>
  <param name="/use_sim_time" value="false"/>
  <arg name="bag" />
  <arg name="rate" default="1.0" />
  <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz0" args="-d $(find launchers)/config/rvizConfig.rviz" />
  <node pkg="rosbag" type="play" name="playback" output="screen" args="$(arg bag) -r $(arg rate) --clock" /> 
</launch>
